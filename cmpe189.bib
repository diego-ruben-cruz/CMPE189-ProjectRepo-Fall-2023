@article{forenbacher_throughput_2021,
  title    = {Throughput of an {IEEE} 802.11 {Wireless} {Network} in the {Presence} of {Wireless} {Audio} {Transmission}: {A} {Laboratory} {Analysis}},
  volume   = {21},
  issn     = {1424-8220},
  doi      = {10.3390/s21082620},
  abstract = {Wireless networks, including IEEE 802.11-based or Wi-Fi networks, are inexpensive and easy to install and therefore serve as useful connectivity alternatives in areas lacking wired-network infrastructure. However, IEEE 802.11 networks may not always provide the seamless connectivity and minimal throughput required for Industry 4.0 communications because of their susceptibility to interference from other devices operating in the unlicensed "Industrial, Scientific, and Medical" frequency band. Here we analyzed how a wireless audio transmitter operating on this band influences the throughput of an IEEE 802.11 b/g/n network under laboratory conditions. Wireless audio transmission reduced mean throughput by 85\%, rendering the IEEE 802.11 b/g/n network nearly unusable. Our analysis suggests that in order for IEEE 802.11 wireless networks to support Industrial 4.0 applications, attention should be paid to the physical layer as well as the data or upper layers, and critical services should not transmit on the 2.4 GHz band. These findings may contribute to understanding and managing IEEE 802.11 wireless networks in various Industry 4.0 contexts.},
  number   = {8},
  journal  = {Sensors (Basel, Switzerland)},
  author   = {Forenbacher, Ivan and Husnjak, Siniša and Jovović, Ivan and Bobić, Mislav},
  year     = {2021},
  note     = {Place: Switzerland
              Publisher: MDPI AG},
  keywords = {Automation, Communication requirements, Energy consumption, Frequencies, IEEE 802.11, Industry 4.0, Laboratories, Microwaves, Multimedia, Non-Wi-Fi interference, Practical implications, Production planning, Telecommunications industry, Throughput measurement, Transmitters, Wireless audio equipment, Wireless communications, Wireless network, Wireless networks},
  pages    = {2620}
}

@article{parks_wireless_2013,
  title    = {Wireless {Audio}},
  volume   = {55},
  issn     = {0007-1994},
  abstract = {Wireless microphones have been used with cameras for years, as they provide a variety of benefits to the production. A concealed transmitter and mic on the talent grabs his or her words at a distance with usable level and intelligibility, in sync with the video and without the masking of extraneous noise. Sight lines and sets are cleaner. A clear wireless signal can prove more robust than long cable runs with multiple connections that create the possibility of induced hum or other signal degradation. The latest generation of wireless mics has drawn on advances in computing, networking and digital audio processing to offer an even more reliable signal - one with a resolution that melds with high-definition visuals provided by new cameras.},
  number   = {6},
  journal  = {Broadcast Engineering},
  author   = {Parks, Gary},
  year     = {2013},
  note     = {Place: Overland Park
              Publisher: Penton Media, Inc., Penton Business Media, Inc. and their subsidiaries},
  keywords = {Transmitters, Wireless communications, Analysis, Bandwidths, Cameras, Communications equipment, Digital transmission, Microphones, Receivers \& amplifiers, Spectrum allocation, Spectrum analysis, Wireless telecommunications equipment},
  pages    = {24}
}

