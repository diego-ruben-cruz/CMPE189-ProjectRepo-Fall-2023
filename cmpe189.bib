
@article{anthony_approach_2021,
  title    = {An {Approach} to {End}-to-{End} {Audio} {Transmission} {Using} {Laser} {Communication}},
  volume   = {118},
  issn     = {0929-6212},
  doi      = {10.1007/s11277-021-08083-1},
  abstract = {Laser Communication is the most evolving arenas in the field of wireless communication. Presently, laser commutation is espoused in the satellite communication for the purpose of space research related activities because of its benefits such as less power consumption, low cost, flexibility and resistance to the radio interferences. This work is an application of laser communication for transmitting data between two ends. Laser Communication needs the transmitter and receiver to be aligned in line-of-sight. The carrier which transmits the signal is produced by a laser diode. The main objective of this work is to transmit audio signal from transmitter to receiver by using the laser beam as a medium. The complete system for this work consists of a transmitter and a reciever capable of transmititng and recieving the analog signal respectively. The results shows that input signal frequencies are preserved at the output with less distortion. However there is decrease in the amplitude of the received signal which can be increased at the output end and the original signal can be recovered. Signal to Noise Ratio is used as a performance metric and it is derived from experimentation that, for higher frequencies Signal to Noise Ratio is more, and for lower frequencies Signal to Noise Ratio is less.},
  number   = {2},
  journal  = {Wireless personal communications},
  author   = {Anthony, Audre Arlene and Basavaiah, Jagadeesh and Patil, Chandrashekar Mohan},
  year     = {2021},
  note     = {Place: New York
              Publisher: Springer US},
  keywords = {Communications Engineering, Commutation, Computer Communication Networks, Demodulation, Engineering, Experimentation, Laser applications, Laser beams, Laser communication, Lasers, Line of sight communication, Modulation, Networks, Optical communication, Power consumption, Reciever, Satellite communications, Semiconductor lasers, Signal to noise ratio, Signal,Image and Speech Processing, Space research, Transmitter, Transmitters, Wireless communications},
  pages    = {1439--1451}
}

